{
  "name": "osrs-reldo-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch server --ext ts,json --exec ts-node ./server/bin/www.ts",
    "start:watch": "nodemon --watch src --ext ts,json --exec ts-node ./src/index.ts",
    "build": "rimraf dist && tsc",
    "start": "NODE_ENV=production node ./dist/bin/www",
    "server": "node ./dist/bin/www",
    "clean": "rimraf dist",
    "test": "jest",
    "init-db": "node ./scripts/createDb.js"
  },
  "dependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.8",
    "@babel/preset-env": "^7.16.8",
    "@babel/runtime": "^7.16.7",
    "aws-sdk": "^2.1387.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^14.1.0",
    "express": "^4.18.2",
    "express-oauth2-jwt-bearer": "^1.5.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "mysql2": "^3.11.4",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "osrs-json-hiscores": "^2.16.0",
    "pg": "^8.13.1",
    "rimraf": "^3.0.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.8",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.1.4",
    "@types/node-fetch": "^2.6.3",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "typescript": "^5.1.6"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/transform-runtime"
      ]
    ]
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
